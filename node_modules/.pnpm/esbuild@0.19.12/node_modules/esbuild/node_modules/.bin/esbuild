#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/private/var/folders/9g/w3sk5dnj47794nzy8ygp3nnw0000gn/T/f7ac8ff111ac5b6c546a6515b6ca175f/app/node_modules/.pnpm/esbuild@0.19.12/node_modules/esbuild/bin/node_modules:/private/var/folders/9g/w3sk5dnj47794nzy8ygp3nnw0000gn/T/f7ac8ff111ac5b6c546a6515b6ca175f/app/node_modules/.pnpm/esbuild@0.19.12/node_modules/esbuild/node_modules:/private/var/folders/9g/w3sk5dnj47794nzy8ygp3nnw0000gn/T/f7ac8ff111ac5b6c546a6515b6ca175f/app/node_modules/.pnpm/esbuild@0.19.12/node_modules:/private/var/folders/9g/w3sk5dnj47794nzy8ygp3nnw0000gn/T/f7ac8ff111ac5b6c546a6515b6ca175f/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/private/var/folders/9g/w3sk5dnj47794nzy8ygp3nnw0000gn/T/f7ac8ff111ac5b6c546a6515b6ca175f/app/node_modules/.pnpm/esbuild@0.19.12/node_modules/esbuild/bin/node_modules:/private/var/folders/9g/w3sk5dnj47794nzy8ygp3nnw0000gn/T/f7ac8ff111ac5b6c546a6515b6ca175f/app/node_modules/.pnpm/esbuild@0.19.12/node_modules/esbuild/node_modules:/private/var/folders/9g/w3sk5dnj47794nzy8ygp3nnw0000gn/T/f7ac8ff111ac5b6c546a6515b6ca175f/app/node_modules/.pnpm/esbuild@0.19.12/node_modules:/private/var/folders/9g/w3sk5dnj47794nzy8ygp3nnw0000gn/T/f7ac8ff111ac5b6c546a6515b6ca175f/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esbuild" "$@"
else
  exec node  "$basedir/../../bin/esbuild" "$@"
fi
